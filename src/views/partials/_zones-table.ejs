<table>
  <thead>
    <tr>
      <th>ID</th>
      <th>Nombre de la Zona</th>
      <th>Acciones</th>
      <th>Hoja de ruta</th>
    </tr>
  </thead>
  <tbody>
    <% zones.forEach(zone => { %>
      <tr>
        <td><%= zone.id %></td>
        <td><%= zone.name %></td>
        <td>
          <a href="/zones/update/<%= zone.id %>">Modificar</a>
        </td>
        <td>
          <a href="/zones/roadmap/<%= zone.id %>">Ver</a>
        </td>
      </tr>
    <% }); %>
  </tbody>
</table>