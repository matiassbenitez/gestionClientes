<table>
  <thead>
    <tr>
      <th>ID</th>
      <th>Nombre</th>
      <th>Ciudad</th>
      <th>Tel√©fono</th>
      <th>Zona</th>
      <th>Acciones</th>
    </tr>
  </thead>
  <tbody>
    <% customers.forEach(customer => { %>
      <tr>
        <td><%= customer.id %></td>
        <td><%= customer.name %></td>
        <td><%= customer.city %></td>
        <td><%= customer.phone_number %></td>
        <td><%= customer.zone_id %></td>
        <td>
          <a class="btn" href="/customers/<%= customer.id %>">Ver</a>
        </td>
      </tr>
    <% }); %>
  </tbody>
</table>